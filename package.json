{
  "name": "markdown-rc-loader",
  "version": "1.1.0-beta.0",
  "description": "Turn Markdown into dynamic, stateless React components",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps inline --ignore \"**/*.spec.js,**/__fixtures__,**/__mocks__\"",
    "test": "jest",
    "build": "yarn prepublish && webpack",
    "start": "webpack-dev-server",
    "lint": "eslint .",
    "lint-and-fixup": "eslint --fix ."
  },
  "keywords": [
    "markdown",
    "markdown-component",
    "markdown-it",
    "component",
    "webpack",
    "react",
    "jsx"
  ],
  "author": "nanyuantingfeng <nanyuantingfeng@aliyun.com>",
  "license": "MIT",
  "dependencies": {
    "any-base": "^1.0.0",
    "doc-chomp": "1.1.0",
    "front-matter": "^2.1.0",
    "hastml": "^0.1.0",
    "he": "^1.1.1",
    "highlight.js": "^9.6.0",
    "jsesc": "^2.2.0",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.1.0",
    "sha.js": "^2.4.5"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-version-inline": "^1.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "codemirror": "^5.30.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.7.2",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "image-webpack-loader": "^3.0.0",
    "jest": "^21.1.0",
    "json-loader": "^0.5.4",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-table-of-contents": "^0.3.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-codemirror2": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.3.2 || ^16.0"
  },
  "babel": {
    "plugins": [
      "version-inline",
      "transform-object-rest-spread"
    ],
    "presets": [
      "es2015",
      "react"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ]
  },
  "directories": {
    "doc": "docs",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanyuantingfeng/markdown-rc-loader.git"
  },
  "bugs": {
    "url": "https://github.com/nanyuantingfeng/markdown-rc-loader/issues"
  },
  "homepage": "https://github.com/nanyuantingfeng/markdown-rc-loader#readme"
}
